{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "type": "object",
    "properties": {
      "username": {
        "type": "string",
        "minLength": 1,
        "maxLength" : 30
      },
      "password": {
        "type": "string",
        "minLength" : 5,
        "maxLength" : 30
      },
      "first_name": {
        "type": "string",
        "minLength" : 1,
        "maxLength": 30
      },
      "last_name": {
        "type": "string",
        "minLength" : 1,
        "maxLength": 30
      },
      "email": {
        "type": "string",
        "minLength" : 6,
        "maxLength" : 60,
        "format" : "email"
      }
    },
    "additionalProperties": false,
    "required": [
      "username",
      "password",
      "first_name",
      "last_name",
      "email"
    ]
  }